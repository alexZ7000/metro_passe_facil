        "expo-font": "~12.0.4",
        "expo-image-picker": "~15.0.5",
        "expo-status-bar": "~1.12.1",
        "jwt-decode": "^4.0.0",
        "react": "18.2.0",
        "react-native": "0.74.1",
        "react-native-collapsible": "^1.6.1",
        "react-native-gesture-handler": "~2.16.1",
        "react-native-mask-input": "^1.2.3",
