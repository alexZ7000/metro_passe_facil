    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "prettier --write .",
            "eslint --fix ."
        ]
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!(@react-native|react-native)/)"
        ]
